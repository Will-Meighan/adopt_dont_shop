<h1>Edit <%= @pet.name %></h1>

<%= form_tag "/pets/#{@pet.id}", method: :PATCH do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %>
  <%= label_tag :image_url %>
  <%= text_field_tag :image %>
  <%= label_tag :description %>
  <%= text_field_tag :description %>
  <%= label_tag :approximate_age %>
  <%= text_field_tag :approximate_age %>
  <%= label_tag :sex %>
  <%= text_field_tag :sex%>
  <%= label_tag :status %>
  <%= text_field_tag :status%>
  <%= submit_tag 'Update Pet' %>
<% end %>
